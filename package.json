    "@tanstack/react-query": "^4.29.3",
    "@tanstack/react-query-devtools": "^4.29.3",
    "@vercel/analytics": "^1.0.0",
    "@vercel/og": "^0.5.3",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "drizzle-orm": "^0.29.3",
    "joi": "^17.9.1",
    "lambda-rate-limiter": "^3.0.1",
    "lodash": "^4.17.21",
    "next": "^13.3.1",
    "next-connect": "^0.13.0",
