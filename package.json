    "next-pwa": "^5.6.0",
    "next-seo": "^6.0.0",
    "next-usequerystate": "^1.7.2",
    "node-fetch": "^3.3.1",
    "pretty-bytes": "^6.1.0",
    "psl": "^1.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.12.0",
    "use-interval": "^1.4.0"
