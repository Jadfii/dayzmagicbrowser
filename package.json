    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.13",
    "eslint": "8.39.0",
    "eslint-config-next": "^13.3.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.23",
    "postgres": "^3.4.4",
    "prettier": "^2.8.7",
    "prettier-plugin-prisma": "^4.12.0",
